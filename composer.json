{
    "name": "michaeljoelphillips/hermes",
    "description": "Hermes, the reliable chatbot",
    "type": "project",
    "require": {
        "php": "^7.4",
        "botman/botman": "^2.6",
        "botman/driver-slack": "^2.2",
        "google/apiclient": "^2.2",
        "guzzlehttp/guzzle": "^6.0",
        "jwilsson/spotify-web-api-php": "^3.6",
        "monolog/monolog": "^2.1",
        "php-di/php-di": "^6.2",
        "php-middleware/log-http-messages": "^4.0",
        "slim/psr7": "^1.2",
        "slim/slim": "^4.5",
        "symfony/cache": "^5.1",
        "symfony/console": "^5.1",
        "symfony/dotenv": "^5.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.1",
        "phpstan/phpstan": "^0.12.58",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
